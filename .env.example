# RAG Food Assistant - Environment Configuration Template
# Copy this file to .env and fill in your actual API keys

# Groq Cloud API Configuration
GROQ_API_KEY=your_groq_api_key_here

# Upstash Vector Database Configuration
UPSTASH_VECTOR_REST_URL=your_upstash_vector_url_here
UPSTASH_VECTOR_REST_TOKEN=your_upstash_vector_token_here
UPSTASH_VECTOR_REST_READONLY_TOKEN=your_upstash_readonly_token_here

# Application Settings
JSON_FILE=foods.json
MAX_RESULTS=3
RESPONSE_TEMPERATURE=0.7
MAX_TOKENS=500

# Optional: Legacy settings (not used in current version)
OLLAMA_HOST=http://localhost:11434
EMBED_MODEL=mxbai-embed-large
LLM_MODEL=llama3.2